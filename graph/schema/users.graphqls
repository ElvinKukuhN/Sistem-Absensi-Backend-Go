# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
  email: String!
  role: String!
  IsActive: String!
  createdAt: String!
  updatedAt: String!
}

input SignUpRequest {
  name: String!
  email: String!
  password: String!
  role: String!
}

type Mutation {
  signUp(input: SignUpRequest!): User!
}

type Query {
  healthCheck: String!
}
